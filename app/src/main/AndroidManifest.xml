<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digitalminds.admindataupload">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_admin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ebooklogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminQSAEBooks">
        <activity android:name="com.digitalminds.admindataupload.CategoryActivity"></activity>
        <activity android:name="com.digitalminds.admindataupload.BookActivity" />
        <activity android:name="com.digitalminds.admindataupload.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>